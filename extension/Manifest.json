{
  "manifest_version": 3,
  "name": "DeepFake Detector",
  "version": "1.0",
  "description": "Detect manipulation on images and classify in fake and real",
  "permissions": [
    "activeTab",
    "downloads",
    "scripting"
    
],
  "action": {
    "default_popup": "Popup.html",
    "default_icon": {
      "16": "images/icons8-16.png",
      "32": "images/icons8-32.png",
      "48": "images/icons8-48.png"
      }
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "icons": {
    "16": "images/icons8-16.png",
      "32": "images/icons8-32.png",
      "48": "images/icons8-48.png"
  }
}